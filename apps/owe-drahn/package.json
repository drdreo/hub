{
    "name": "owe-drahn",
    "version": "2.0.1",
    "private": true,
    "nx": {
        "targets": {
            "deploy": {
                "executor": "nx:run-commands",
                "dependsOn": ["build"],
                "inheritEnvironment": true,
                "options": {
                    "parallel": false
                },
                "configurations": {
                    "production": {
                        "commands": [
                            "wrangler pages deploy dist/apps/{projectName}/browser --project-name={projectName} --branch=$NX_BRANCH --commit-dirty=true"
                        ]
                    }
                },
                "defaultConfiguration": "production"
            }
        }
    }
}