{
    "jsc": {
        "target": "es2021",
        "parser": {
            "syntax": "typescript",
            "decorators": true,
            "dynamicImport": true
        },
        "transform": {
            "decoratorMetadata": true,
            "legacyDecorator": true
        },
        "keepClassNames": true,
        "externalHelpers": true,
        "loose": true
    },
    "module": {
        "type": "es6"
    },
    "sourceMaps": true,
    "exclude": []
}
